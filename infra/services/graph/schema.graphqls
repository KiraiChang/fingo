# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  name: String!
  pwd: String!
  age: Int!
  email: String!
}

type Query {
  users: [User!]!
}

input NewUser {
  name: String!
  pwd: String!
  age: Int!
  email: String!
}

type Mutation {
  createUser(input: NewUser!): User!
}